<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageViewLogo"
        android:layout_width="297dp"
        android:layout_height="168dp"
        android:layout_marginStart="56dp"
        android:scaleType="centerCrop"
        android:src="@drawable/Restaurante"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="-3dp" />

    <ImageView
        android:id="@+id/imageViewPastel"
        android:layout_width="144dp"
        android:layout_height="116dp"
        android:layout_marginStart="56dp"
        android:scaleType="centerCrop"
        android:src="@drawable/PasteldeChoclo"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="177dp" />

    <ImageView
        android:id="@+id/imageViewcazuela"
        android:layout_width="143dp"
        android:layout_height="136dp"
        android:layout_marginStart="56dp"
        android:scaleType="centerCrop"
        android:src="@drawable/Cazuela"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="332dp" />


    <TextView
        android:id="@+id/textViewComida"
        android:layout_width="88dp"
        android:layout_height="18dp"
        android:text="Comida: $"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="539dp" />

    <Switch
        android:id="@+id/checkBoxPropina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Propina"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="557dp" />

    <TextView
        android:id="@+id/textViewTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total:"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="584dp" />

    <TextView
        android:id="@+id/textViewPastel"
        android:layout_width="167dp"
        android:layout_height="37dp"
        android:text="Pastel de Choclo"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="177dp" />

    <TextView
        android:id="@+id/textViewCazuela"
        android:layout_width="167dp"
        android:layout_height="37dp"
        android:text="Cazuela"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="332dp" />

    <TextView
        android:id="@+id/textViewPrecio"
        android:layout_width="88dp"
        android:layout_height="18dp"
        android:text="$10.000"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="429dp" />

    <TextView
        android:id="@+id/textViewPrecio"
        android:layout_width="96dp"
        android:layout_height="30dp"
        android:text="$12.000"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="257dp" />

</androidx.constraintlayout.widget.ConstraintLayout>